{
  "name": "expresstypescriptstarter",
  "version": "1.0.0",
  "description": "Express Typescript Starter",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev:complete": "nodemon --watch src -e ts --exec npm run dev:start",
    "dev": "nodemon --config \"./nodemon.json\"/",
    "start": "node ./dist/server.js",
    "test": "none"
  },
  "author": "Jean Paul Saucedo",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.12",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "sequelize": "^5.19.5"
  }
}
